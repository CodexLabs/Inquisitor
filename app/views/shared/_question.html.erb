<div class="question">
  <div class="prompt">
    <b>
      <%=index + 1%>)
      <i><%= question.prompt %></i>
    </b>
  </div>
  <%- question.randomized_answers.each do |answer|%>
    <div class="answer">
      <%=radio_button_tag "question[#{question.id}]",answer%> 
      <%= answer%>
    </div>
  <%- end%>
</div>